    {
      "name": "Setup - Authentication",
      "item": [
        {
          "name": "1. Register User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "  let data = pm.response.json().data;",
                  "  pm.environment.set('user_id', data.user_id);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"vocab-teacher@example.com\",\n  \"password\": \"SecurePass123!\",\n  \"display_name\": \"Vocabulary Teacher\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/auth/register/email",
              "host": ["{{base_url}}"],
              "path": ["auth", "register", "email"]
            }
          }
        },
        {
          "name": "2. Login User",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "  let data = pm.response.json().data;",
                  "  pm.environment.set('access_token', data.access_token);",
                  "  pm.environment.set('refresh_token', data.refresh_token);",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
