            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"vocab-teacher@example.com\",\n  \"password\": \"SecurePass123!\",\n  \"device_id\": \"device-001\",\n  \"device_fingerprint\": \"fingerprint-001\"\n}"
